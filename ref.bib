@article{yijianzhanguoshiqibaleng,
  author        = {王婕 and 李沫 and 马清林 and 张治国 and 章梅芳 and 王菊琳},
  title         = {一件战国时期八棱柱状铅钡玻璃器的风化研究},
  journal       = {玻璃与搪瓷},
  year          = {2014},
  authoraddress = {北京科技大学冶金与材料史研究所;北京化工大学北京市电化学过程与技术重点实验室;中国文化遗产研究院;},
  volume        = {42},
